<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bsuir.controller.HistogramController">
   <children>
      <BarChart fx:id="histogram" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0">
        <xAxis>
          <CategoryAxis fx:id="axisX" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="axisY" side="LEFT" upperBound="10.0" />
        </yAxis>
      </BarChart>
   </children>
</AnchorPane>
